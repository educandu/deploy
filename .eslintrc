{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended"
  ],
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": [
    "import"
  ]
}
